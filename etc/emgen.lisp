(defun int (n)
  `((string ,(format nil "~A" n))
    (string->number)))

(defun get-var (name)
  `((string ,(format nil "~A" name))
    (retrieve)))

(defun set-var (name)
  `((string ,(format nil "~A" name))
    (store)))

(defun drop ()
